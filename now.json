{
  "name": "thoughtsapp",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "build": {
    "env": {
      "PORT": "@ta-port",
      "AUTH0_DOMAIN": "@ta-domain",
      "AUTH0_CLIENT_ID": "@ta-client-id",
      "AUTH0_CLIENT_SECRET": "@ta-client-secret",
      "CALLBACK_URL": "@ta-callback-url",
      "BASE_URL": "@ta-base-url"
    }
  }
}